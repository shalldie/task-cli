{
  "name": "todo-cli-js",
  "version": "0.0.6",
  "description": "A todo manager on the command-line interface.",
  "author": "shalldie @shalldie",
  "bin": {
    "todo": "./bin/run"
  },
  "homepage": "https://github.com/shalldie/todo-cli-js",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "shalldie/todo-cli-js",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "chalk": "^4.1.2",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "chai": "^4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^2",
    "prettier": "^2.6.1",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "oclif": {
    "bin": "todo",
    "dirname": "todo",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc && oclif manifest",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/shalldie/todo-cli-js/issues",
  "keywords": [
    "todo cli"
  ],
  "types": "dist/index.d.ts"
}
